<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center"
    android:background="#fffdf6"
    android:orientation="vertical">
    <ImageView
        android:id="@+id/zhuceyebtn-1"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="20dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="17dp"
        android:background="@android:color/transparent"
        app:srcCompat="@drawable/shizhizhuye3" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_alignBottom="@+id/zhuceyebtn-1"
        android:layout_alignTop="@+id/zhuceyebtn-1"
        android:layout_centerHorizontal="true"
        app:srcCompat="@drawable/zhuche" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_alignLeft="@+id/zhuceyebtn-1"
        android:layout_alignStart="@+id/zhuceyebtn-1"
        android:layout_below="@+id/zhuceyebtn-1"
        android:layout_marginTop="57dp"
        app:srcCompat="@drawable/personal2" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/imageView7"
        android:layout_marginTop="55dp"
        app:srcCompat="@drawable/zhuceye1" />

    <EditText
        android:id="@+id/zhuceedit-1"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_alignBottom="@+id/imageView11"
        android:layout_toEndOf="@+id/imageView11"
        android:layout_toRightOf="@+id/imageView11"
        android:ems="10"
        android:inputType="phone" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_alignLeft="@+id/imageView7"
        android:layout_alignStart="@+id/imageView7"
        android:layout_below="@+id/imageView11"
        android:layout_marginTop="57dp"
        app:srcCompat="@drawable/dlock" />

    <EditText
        android:id="@+id/zhuceedit-2"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_above="@+id/zhuceedit-3"
        android:layout_toEndOf="@+id/imageView11"
        android:layout_toRightOf="@+id/imageView11"
        android:ems="10"
        android:inputType="textPassword" />

    <EditText
        android:id="@+id/zhuceedit-3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/imageView12"
        android:layout_alignStart="@+id/imageView12"
        android:layout_below="@+id/imageView12"
        android:layout_marginTop="39dp"
        android:ems="10"
        android:hint="请输入验证码"
        android:inputType="textPersonName" />

    <ImageView
        android:id="@+id/zhuceyebtn-2"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_alignBottom="@+id/zhuceedit-3"
        android:layout_toEndOf="@+id/zhuceedit-3"
        android:layout_toRightOf="@+id/zhuceedit-3"
        android:background="#00000000"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/fasong" />

</RelativeLayout>
