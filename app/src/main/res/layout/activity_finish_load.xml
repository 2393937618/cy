<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center"
    android:orientation="vertical"
    android:background="#fffdf6">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:orientation="horizontal"
        android:id="@+id/linearLayout2">


        <ImageView
            android:id="@+id/yidenglu-1"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="10dp"
            android:background="@android:color/transparent"
            app:srcCompat="@drawable/personal4" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="85dp"
            android:text="个人中心"
            android:textSize="20sp" />

        <ImageView
            android:id="@+id/yidenglu-2"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginLeft="90dp"
            android:layout_marginTop="10dp"
            android:background="@android:color/transparent"
            app:srcCompat="@drawable/personal21" />


    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/linearLayout2"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/linearLayout3-1"
            android:layout_width="match_parent"
            android:layout_height="104dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/linearLayout2"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="104dp"
                android:orientation="horizontal">


                <ImageView
                    android:id="@+id/douxian"
                    android:layout_width="97dp"
                    android:layout_height="93dp"
                    android:layout_marginLeft="40dp"
                    android:layout_marginTop="10dp"
                    android:adjustViewBounds="true"
                    android:contentDescription="@string/app_name"
                    android:background="@color/colorPrimary"
                     />

                <TextView
                    android:id="@+id/username_data"
                    android:layout_width="138dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="30dp"
                    android:text="用户名"
                    android:textSize="22sp" />

                <ImageView
                    android:id="@+id/yidenglu-3"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_marginLeft="30dp"
                    android:layout_marginTop="20dp"
                    android:adjustViewBounds="true"
                    app:srcCompat="@drawable/yidenglu22" />

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="20dp"
                android:layout_marginLeft="260dp"
                android:layout_weight="1"
                android:adjustViewBounds="true"
                app:srcCompat="@drawable/yidenglu10" />

        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/linearLayout3"
        android:orientation="horizontal"
        android:id="@+id/linearLayout4">

        <LinearLayout
            android:id="@+id/fensi"
            android:layout_width="111dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="25dp"
            android:orientation="vertical">

            <TextView
                android:id="@+id/fans_data"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="0"
                android:textSize="25sp" />

            <ImageView
                android:id="@+id/yidenglu-8-1"
                android:layout_width="match_parent"
                android:layout_height="21dp"
                android:background="@android:color/transparent"
                app:srcCompat="@drawable/personal9" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/guanzhu"
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/attentions_data"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:gravity="center"
                android:text="0"
                android:textSize="25sp" />

            <ImageView
                android:id="@+id/yidenglu-9-1"
                android:layout_width="match_parent"
                android:layout_height="21dp"
                android:background="@android:color/transparent"
                app:srcCompat="@drawable/personal12" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/shouchang"
            android:layout_width="117dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="0dp"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageview5"
                android:layout_width="match_parent"
                android:layout_height="25dp"
                android:layout_marginTop="20dp"
                android:background="@android:color/transparent"
                app:srcCompat="@drawable/personal1" />

            <ImageView
                android:id="@+id/yidenglu-10"
                android:layout_width="match_parent"
                android:layout_height="21dp"
                android:layout_marginTop="15dp"
                android:background="@android:color/transparent"
                app:srcCompat="@drawable/personal15" />
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="350dp"
        android:layout_height="140dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/linearLayout4"
        android:orientation="vertical"
        android:background="@drawable/mybackground"
        android:id="@+id/linearLayout5">

        <TextView
            android:id="@+id/textView4"
            android:layout_width="98dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="25dp"
            android:text="我的旅行"
            android:textSize="20sp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="53dp"
            android:layout_marginTop="15dp"
            android:layout_marginLeft="20dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/yidenglu-5"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_marginLeft="60dp"
                app:srcCompat="@drawable/personal20" />

            <ImageView
                android:id="@+id/yidenglu-6"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_marginLeft="50dp"
                app:srcCompat="@drawable/personal16" />

            <ImageView
                android:id="@+id/yidenglu-7"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_marginLeft="50dp"
                app:srcCompat="@drawable/personal14" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="5dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/yidenglu-5-1"
                android:layout_width="40dp"
                android:layout_height="28dp"
                android:layout_marginLeft="110dp"
                android:layout_marginTop="5dp"
                android:background="@android:color/transparent"
                app:srcCompat="@drawable/personal22" />

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="wrap_content"
                android:layout_height="35dp"
                android:layout_marginLeft="15dp"
                app:srcCompat="@drawable/personal10" />

            <ImageView
                android:id="@+id/yidenglu-6-1"
                android:layout_width="41dp"
                android:layout_height="27dp"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="5dp"
                android:background="@android:color/transparent"
                app:srcCompat="@drawable/personal17" />

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="wrap_content"
                android:layout_height="35dp"
                android:layout_marginLeft="15dp"
                app:srcCompat="@drawable/personal10" />

            <ImageView
                android:id="@+id/yidenglu-7-1"
                android:layout_width="39dp"
                android:layout_height="27dp"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="5dp"
                android:background="@android:color/transparent"
                app:srcCompat="@drawable/personal22" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/linearLayout5"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textView5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="10dp"
            android:text="更多服务"
            android:textSize="20sp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="5dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/yidenglu-13"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginTop="10dp"
                android:background="@android:color/transparent"
                app:srcCompat="@drawable/personal5" />
        </LinearLayout>
    </LinearLayout>



</RelativeLayout>
