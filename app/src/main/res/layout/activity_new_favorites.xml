<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fffdf6">


    <ImageView
        android:id="@+id/xinjianshouchangjiabtn1"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="20dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="17dp"
        android:background="@android:color/transparent"
        app:srcCompat="@drawable/shizhizhuye3" />

    <ImageView
        android:id="@+id/imageView24"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_alignTop="@+id/xinjianshouchangjiabtn1"
        android:layout_centerHorizontal="true"
        app:srcCompat="@drawable/xinjianshouchangjia11" />

    <ImageView
        android:id="@+id/imageView25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/xinjianshouchangjiabtn1"
        app:srcCompat="@drawable/xinjianshouchangjia5" />

    <ImageView
        android:id="@+id/imageView26"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/imageView24"
        android:layout_marginTop="62dp"
        app:srcCompat="@drawable/xinjianshouchangjia8" />

    <ImageView
        android:id="@+id/xinjianshouchangjiabtn2"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_alignEnd="@+id/xinjianshouchangjiabtn4"
        android:layout_alignRight="@+id/xinjianshouchangjiabtn4"
        android:layout_below="@+id/imageView26"
        android:layout_marginTop="77dp"
        android:background="@android:color/transparent"
        android:elevation="0dp"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/xinjianshouchangjia1" />

    <ImageView
        android:id="@+id/imageView27"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/xinjianshouchangjiabtn2"
        app:srcCompat="@drawable/xinjianshouchangjia9" />

    <EditText
        android:id="@+id/xinjianshouchangjiaedit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/xinjianshouchangjiabtn1"
        android:layout_alignStart="@+id/xinjianshouchangjiabtn1"
        android:layout_below="@+id/imageView27"
        android:ems="10"
        android:inputType="textPersonName" />

    <ImageView
        android:id="@+id/xinjianshouchangjiabtn3"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="14dp"
        android:layout_toLeftOf="@+id/imageView24"
        android:layout_toStartOf="@+id/imageView24"
        android:background="@android:color/transparent"
        app:srcCompat="@drawable/xinjianshouchangjia7" />

    <ImageView
        android:id="@+id/xinjianshouchangjiabtn4"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_alignBottom="@+id/xinjianshouchangjiabtn3"
        android:layout_toEndOf="@+id/imageView24"
        android:layout_toRightOf="@+id/imageView24"
        android:background="@android:color/transparent"
        app:srcCompat="@drawable/xinjianshouchangjia10" />
</RelativeLayout>
