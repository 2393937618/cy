<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fffdf6">

    <ImageView
        android:id="@+id/imageView40"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_toLeftOf="@+id/wendatext4"
        android:layout_toStartOf="@+id/wendatext4"
        android:adjustViewBounds="true"
        app:srcCompat="@drawable/wenda8" />

    <TextView
        android:id="@+id/wendatext1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="26dp"
        android:layout_marginStart="26dp"
        android:layout_toEndOf="@+id/imageView40"
        android:layout_toRightOf="@+id/imageView40"
        android:text="shijian"
        android:textSize="18sp" />

    <ImageView
        android:id="@+id/imageView44"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_below="@+id/wendatext1"
        android:layout_marginTop="11dp"
        android:layout_toLeftOf="@+id/wendatext1"
        android:layout_toStartOf="@+id/wendatext1"
        android:adjustViewBounds="true"
        app:srcCompat="@drawable/wenda11" />

    <TextView
        android:id="@+id/wendatext2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/imageView45"
        android:layout_alignLeft="@+id/wendatext1"
        android:layout_alignStart="@+id/wendatext1"
        android:text="wenti"
        android:textSize="18sp" />

    <ImageView
        android:id="@+id/imageView45"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/imageView44"
        android:layout_marginTop="18dp"
        android:adjustViewBounds="true"
        app:srcCompat="@drawable/wenda12" />

    <TextView
        android:id="@+id/wendatext3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/imageView45"
        android:layout_marginLeft="19dp"
        android:layout_marginStart="19dp"
        android:layout_toEndOf="@+id/wendatext1"
        android:layout_toRightOf="@+id/wendatext1"
        android:text="huida"
        android:textSize="20sp" />

    <ImageView
        android:id="@+id/imageView46"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/imageView45"
        android:layout_marginTop="11dp"
        android:adjustViewBounds="true"
        app:srcCompat="@drawable/wenda4" />

    <TextView
        android:id="@+id/wendatext4"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_alignEnd="@+id/imageView45"
        android:layout_alignRight="@+id/imageView45"
        android:layout_alignTop="@+id/imageView46"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:text="didian"
        android:textSize="18sp" />

    <ImageView
        android:id="@+id/imageView42"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/imageView46"
        app:srcCompat="@drawable/fensi4" />

    <ImageView
        android:id="@+id/imageButton"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_alignBottom="@+id/wendatext4"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:background="@android:color/transparent"
        app:srcCompat="@drawable/wenda1" />

</RelativeLayout>
