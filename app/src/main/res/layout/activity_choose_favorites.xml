<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fffdf6">

    <ImageView
        android:id="@+id/xuanzeshouchangjiabtn1"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="20dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="17dp"
        android:background="@android:color/transparent"
        app:srcCompat="@drawable/shizhizhuye3" />

    <ImageView
        android:id="@+id/imageView37"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_alignBottom="@+id/xuanzeshouchangjiabtn1"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:adjustViewBounds="true"
        app:srcCompat="@drawable/xuanzeshouchangjia6" />

    <ImageView
        android:id="@+id/xuanzeshouchangjiabtn2"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginBottom="14dp"
        android:layout_marginEnd="14dp"
        android:layout_marginRight="14dp"
        android:adjustViewBounds="true"
        android:background="@android:color/transparent"
        app:srcCompat="@drawable/xuanzeshouchangjia3" />

    <ListView
        android:id="@+id/xuanzeshouchangjialist"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/xuanzeshouchangjiabtn2"
        android:layout_alignLeft="@+id/xuanzeshouchangjiabtn1"
        android:layout_alignStart="@+id/xuanzeshouchangjiabtn1"
        android:layout_below="@+id/imageView37"
        android:layout_marginTop="18dp" />

</RelativeLayout>
