<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fffdf6">

    <ImageView
        android:id="@+id/yinsishizhibtn1"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="20dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="17dp"
        android:background="@android:color/transparent"
        app:srcCompat="@drawable/shizhizhuye3" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_alignTop="@+id/yinsishizhibtn1"
        android:layout_centerHorizontal="true"
        app:srcCompat="@drawable/yinsishizhi7" />

    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/imageView14"
        android:layout_marginTop="15dp"
        app:srcCompat="@drawable/tongzhishizhi4" />

    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_below="@+id/imageView17"
        android:layout_marginTop="25dp"
        android:layout_toLeftOf="@+id/yinsishizhibtn2"
        android:layout_toStartOf="@+id/yinsishizhibtn2"
        app:srcCompat="@drawable/yinsishizhi8" />

    <ImageView
        android:id="@+id/yinsishizhibtn2"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_above="@+id/imageView20"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        app:srcCompat="@drawable/tongzhishizhi1" />

    <ImageView
        android:id="@+id/imageView20"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/imageView18"
        android:layout_marginTop="17dp"
        app:srcCompat="@drawable/yinsishizhi5" />
</RelativeLayout>

