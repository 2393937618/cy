<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fffdf6">

    <ImageView
        android:id="@+id/beijintouxianview"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="95dp"
        app:srcCompat="@drawable/gerenxinxi11" />

    <ImageView
        android:id="@+id/beijintouxianbtn-1"
        android:layout_width="100dp"
        android:layout_height="25dp"
        android:layout_below="@+id/beijintouxianview"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="73dp"
        android:background="@android:color/transparent"
        app:srcCompat="@drawable/beijintouxian4" />

    <ImageView
        android:id="@+id/beijintouxianbtn-2"
        android:layout_width="180dp"
        android:layout_height="25dp"
        android:layout_below="@+id/beijintouxianbtn-1"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="18dp"
        android:background="@android:color/transparent"
        app:srcCompat="@drawable/beijintouxian1" />

    <ImageView
        android:id="@+id/beijintouxianbtn-3"
        android:layout_width="80dp"
        android:layout_height="25dp"
        android:layout_below="@+id/beijintouxianbtn-2"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="16dp"
        android:background="@android:color/transparent"
        app:srcCompat="@drawable/beijintouxian5" />
</RelativeLayout>
