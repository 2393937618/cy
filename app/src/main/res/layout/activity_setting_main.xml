<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F7F6FB">

    <ImageView
        android:id="@+id/setting_main_return"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="20dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="17dp"
        android:background="@android:color/transparent"
        app:srcCompat="@drawable/shizhizhuye3" />


    <TextView
        android:id="@+id/setting_main_text"
        android:layout_width="50dp"
        android:layout_height="25dp"
        android:text="设置"
        android:textSize="18sp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="16dp"/>





    <LinearLayout
        android:id="@+id/setting_main_password"
        android:layout_width="320dp"
        android:layout_height="30dp"
        android:orientation="horizontal"
        android:layout_below="@+id/setting_main_text"
        android:layout_marginTop="30dp"
        android:layout_centerHorizontal="true"
        android:gravity="left">

        <TextView
            android:layout_width="80dp"
            android:layout_height="20dp"
            android:text="设置密码"
            android:layout_gravity="center"
            android:layout_weight="1"
            />

        <ImageView
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:src="@drawable/shizhizhuye7"
            android:layout_gravity="center"
            />


    </LinearLayout>


    <LinearLayout
        android:id="@+id/setting_main_intent"
        android:layout_width="320dp"
        android:layout_height="30dp"
        android:orientation="horizontal"
        android:layout_below="@+id/setting_main_password"
        android:layout_marginTop="30dp"
        android:layout_centerHorizontal="true"
        android:gravity="left">

        <TextView
            android:layout_width="80dp"
            android:layout_height="20dp"
            android:text="网络诊断"
            android:layout_gravity="center"
            android:layout_weight="1"
            />

        <ImageView
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:src="@drawable/shizhizhuye7"
            android:layout_gravity="center"
            />


    </LinearLayout>


    <LinearLayout
        android:id="@+id/setting_main_userAgreement"
        android:layout_width="320dp"
        android:layout_height="30dp"
        android:orientation="horizontal"
        android:layout_below="@+id/setting_main_intent"
        android:layout_marginTop="30dp"
        android:layout_centerHorizontal="true"
        >

        <TextView
            android:layout_width="80dp"
            android:layout_height="20dp"
            android:text="用户协议"
            android:layout_gravity="center"
            android:layout_weight="1"
            />

        <ImageView
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:src="@drawable/shizhizhuye7"
            android:layout_gravity="center"
            />


    </LinearLayout>


    <LinearLayout
        android:id="@+id/setting_main_edition"
        android:layout_width="320dp"
        android:layout_height="30dp"
        android:orientation="horizontal"
        android:layout_below="@+id/setting_main_userAgreement"
        android:layout_marginTop="30dp"
        android:layout_centerHorizontal="true"
        android:gravity="left">

        <TextView
            android:layout_width="80dp"
            android:layout_height="20dp"
            android:text="关于畅游"
            android:layout_gravity="center"
            android:layout_weight="1"
            />

        <ImageView
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:src="@drawable/shizhizhuye7"
            android:layout_gravity="center"
            />


    </LinearLayout>

    <TextView
        android:id="@+id/setting_main_cancellation"
        android:layout_width="200dp"
        android:layout_height="30dp"
        android:layout_below="@+id/setting_main_edition"
        android:layout_centerHorizontal="true"
        android:text="注销此账户"
        android:textAlignment="center"
        android:textSize="15sp"
        android:layout_marginTop="20dp"
        />





